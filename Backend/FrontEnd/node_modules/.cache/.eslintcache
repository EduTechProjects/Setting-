m \"../utils/Palette\";\nimport { ReactComponent as Caution } from \"../assets/caution.svg\";\nimport axios from \"axios\";\nimport Button from \"../components/Common/Button\";\n\nconst Subjectcontainer = styled.div`\n  background-color: #f5f5f5;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n`;\n\nconst Title = styled.div`\n  font-size: 32px;\n  font-weight: bold;\n  padding-top: 15vh;\n  padding-bottom: 5vh;\n`;\n\nconst Document = styled.div`\n  font-size: 16px;\n  background-color: white;\n  width: 37vw;\n  justify-content: center;\n  align-items: center;\n  border-radius: 28px;\n  margin-bottom: 5vh;\n  padding-bottom: 4vh;\n`;\n\nconst Cautionbox = styled.div`\n  display: inline-flex;\n  font-size: 14px;\n  color: gray;\n`;\nconst SubjectButtons = styled.div`\n  display: flex;\n  padding-top: 5vh;\n`;\n\nconst SubjectTitle = styled.div`\n  font-size: 20px;\n  font-weight: bold;\n  padding-top: 5vh;\n`;\n\nconst DocumentBox = styled.div`\n  width: 80%;\n  text-align: justify;\n  text-align-last: left;\n  margin: 0 auto;\n  line-height: 170%;\n`;\n\nconst CautionIcon = styled(Caution)`\n  width: 15px;\n  height: 15px;\n`;\n\nconst BtnContainer = styled.div`\n  margin-top: 100px;\n  margin-bottom: 100px;\n  margin-left: -30px;\n`;\n\nconst Subject = () => {\n  const navigate = useNavigate();\n  const [selectTopic, setSelectTopic] = useState(null);\n\n  const handleSubButtonClick = (index) => {\n    setSelectTopic(index);\n  };\n\n  const handleStartButtonClick = () => {\n    if (selectTopic !== null) {\n      let topic;\n      if (selectTopic === 1) {\n        topic = \"food\";\n      } else if (selectTopic === 2) {\n        topic = \"travel\";\n      } else if (selectTopic === 3) {\n        topic = \"family\";\n      }\n      // 선택한\n      navigate(`/speak/${topic}`);\n    } else {\n      alert(\"토픽을 설정해주세요.\");\n    }\n  };\n\n  return (\n    <>\n      <Subjectcontainer>\n        <Title style={{ color: \"black\" }}>\n          <b style={{ color: ColorCode.SelectBlue }}>토킹</b> 메이트 시작하기\n        </Title>\n        <Document>\n          <p style={{ fontWeight: \"bold\", fontSize: \"24px\" }}>사용 설명서</p>\n          <DocumentBox>\n            <p>\n              1. 아래 3개의 주제 중 하나를 골라주세요.\n              <br />\n              2. 선택한 주제와 관련된 질문 3개가 제공됩니다.\n              <br />\n              3. 각 질문에 영어로 답변해주세요. (제한 시간 2분)\n              <br />\n              4. START 버튼을 누른 뒤 답변을 시작하고, 답변이 끝나면 END 버튼을\n              눌러주세요.\n              <br />\n              5. 모든 질문에 대한 답변이 끝나면 피드백이 제공됩니다.\n              <br />\n            </p>\n          </DocumentBox>\n          <Cautionbox>\n            <CautionIcon />\n            질문당 권장 답변 시간인 2분에 맞추어 설정했습니다.\n          </Cautionbox>\n        </Document>\n        <SubjectTitle>주제를 선택해주세요.</SubjectTitle>\n        <SubjectButtons>\n          <Button\n            type={selectTopic === 1 ? \"2\" : \"4\"}\n            text={\"음식\"}\n            click={() => handleSubButtonClick(1)}\n          ></Button>\n          <Button\n            type={selectTopic === 2 ? \"2\" : \"4\"}\n            text={\"여행\"}\n            click={() => handleSubButtonClick(2)}\n          ></Button>\n          <Button\n            type={selectTopic === 3 ? \"2\" : \"4\"}\n            text={\"가족\"}\n            click={() => handleSubButtonClick(3)}\n          ></Button>\n        </SubjectButtons>\n        <BtnContainer>\n          <Button type={\"1\"} text={\"시작하기\"} click={handleStartButtonClick} />\n        </BtnContainer>\n      </Subjectcontainer>\n    </>\n  );\n};\n\nexport default Subject;\n","/Users/root1/eduproject/Backend/FrontEnd/src/pages/Feedback.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/components/Common/Navbar.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/components/Common/Button.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/components/Speak/Speaker.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/utils/Palette.js",[]