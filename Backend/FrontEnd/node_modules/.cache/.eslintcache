\n        </A>\n      </AContainer>\n      {!isToggleon && (\n        <React.Fragment>\n          <div style={{ display: \"flex\", flexDirection: \"column\" }}>\n            <AnswerContainer answer={answer} />\n            <FeedbackContainer feedback={feedback} />\n          </div>\n        </React.Fragment>\n      )}\n    </Container>\n  );\n};\n\nexport default ToggleBox;\n","/Users/root1/eduproject/Backend/FrontEnd/src/components/Home/Dropdownlist.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/components/Feedback/AnswerContainer.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/components/Feedback/FeedbackContainer.js",[],[],"/Users/root1/eduproject/Backend/FrontEnd/src/service-worker.js",[],["96","97","98","99"],{"ruleId":"100","severity":1,"message":"101","line":7,"column":8,"nodeType":"102","messageId":"103","endLine":7,"endColumn":13},{"ruleId":"100","severity":1,"message":"104","line":3,"column":20,"nodeType":"102","messageId":"103","endLine":3,"endColumn":29},{"ruleId":"105","severity":2,"message":"106","line":11,"column":1,"nodeType":"102","messageId":"107","endLine":11,"endColumn":5,"suppressions":"108"},{"ruleId":"105","severity":2,"message":"106","line":21,"column":1,"nodeType":"102","messageId":"107","endLine":21,"endColumn":5,"suppressions":"109"},{"ruleId":"105","severity":2,"message":"106","line":36,"column":1,"nodeType":"102","messageId":"107","endLine":36,"endColumn":5,"suppressions":"110"},{"ruleId":"105","severity":2,"message":"106","line":49,"column":18,"nodeType":"102","messageId":"107","endLine":49,"endColumn":22,"suppressions":"111"},"no-unused-vars","'axios' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used.","no-restricted-globals","Unexpected use of 'self'.","defaultMessage",["112"],["113"],["114"],["115"],{"kind":"116","justification":"117"},{"kind":"116","justification":"117"},{"kind":"116","justification":"117"},{"kind":"116","justification":"117"},"directive",""]